#include <iostream>
#include <fstream>
using namespace std;
struct person1
{
	int numbe[10];
	char name[10];
}person1[5];
struct person2
{
	int score[10];
	int numbe[10];
	char name[10];
}person2[5];
int main(int argc, const char * argv[])
{
    int a,b,i,j;
    ifstream in1("C:/Users/杨杨/Desktop/1.txt");
    ifstream in2("C:/Users/杨杨/Desktop/2.txt");
    ofstream out("C:/Users/杨杨/Desktop/3.txt");
    if(in1.is_open())
    {
        cout<<"file ok"<<endl;
        for(i=0;i<5;i++)
        {
        	for(j=0;j<5;j++)
        	{
		in1>>person1[i].numbe;
	        }
        in1>>person1[i].name;
        }
        cout<<a<<" "<<endl;
        in1.close();
    }
    if(in2.is_open())
    {
		cout<<"file ok"<<endl;
		for(i=0;i<5;i++)
        {
        	for(j=0;j<5;j++)
        	{
        in2>>person2[i].score;
        in2>>person2[i].numbe;
		}
		in2>>person2[i].name;
	    }
        cout<<person2<<" "<<endl;
        int i,j,n,pj=0,h=0;
    cin>>n;
	for(i=0;i<n;i++)
	{
      cin>>person2[i].score;
    }
    for(j=0;j<n;j++)
	{
    for(i=0;i<n;i++)
	   {
       if(person2[i].score>person2[i+1].score)
	   {
         person2[n].score=person2[i].score;
        person2[i].score=person2[i+1].score;
        person2[i+1].score=person2[n].score;
		}
	}
	}
    for(i=0;i<n;i++)
    {
	    cout<<person2[i].score<<" "<<endl;
    }
    for(i=1;i<n-1;i++)
    {
	    h=person2[i].score+h;
	    pj=h/(n-2);
    }
	cout<<pj<<endl;
        in2.close();  
    }
    if(out.is_open())
	    {
        
        out.close();
        }
	return 0;
	}



