#include <iostream>
#include <fstream>
using namespace std;
struct person1
{
    char  numbe[10];
    char name[10];
}person1[5];
struct person2
{
    int score[10];
    char numbe[10];
    char name[10];
}person2[5];
int main(int argc, const char * argv[])
{
    int i,j;
    ifstream in1("/Users/s20181106117/Desktop/caipan.txt ");
    ifstream in2("/Users/s20181106117/Desktop/caipan-2.txt ");
    ofstream out("/Users/s20181106117/Desktop/chengji.txt ");
    if(in1.is_open())
        {
        for(i=0;i<5;i++)
        {
            for(j=0;j<5;j++)
            {
                in1>>person1[i].numbe;
            }
            in1>>person1[i].name;
        }
        cout<<person1<<" "<<endl;
        in1.close();
    }
    if(in2.is_open())
        {
        for(i=0;i<5;i++)
            {
            for(j=0;j<5;j++)
                {
                    for(int z=0;z<5;z++)
                    {
                in2>>person2[i].score[z];
                    }
                in2>>person2[i].numbe;
                }
          in2>>person2[i].name;
        }
        cout<<person2<<" "<<endl;
        int i,j,n,average=0,h=0;
        cin>>n;
        for(i=0;i<n;i++)
        {
            cin>>person2[i].score[i];
        }
        for(j=0;j<n;j++)
            {
            for(i=0;i<n;i++)
                {
                if(person2[i].score>person2[i+1].score)
                    {
                    person2[n].score[n]=person2[i].score[i];
                    person2[i].score[i]=person2[i+1].score[i+1];
                    person2[i+1].score[n+1]=person2[n].score[n];
                    }
                }
             }
        for(i=0;i<n;i++)
            {
            cout<<person2[i].score<<" "<<endl;
            }
        for(i=1;i<n-1;i++)
            {
            h=person2[i].score[i]+h;
            }
            average=h/(n-2);
        cout<<average<<endl;
        in2.close();
        }
    if(out.is_open())
    {
        out.close();
    }
    return 0;
    }
